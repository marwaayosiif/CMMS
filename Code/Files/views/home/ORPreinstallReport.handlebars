                                        <div class="col-lg-10">
                                            <p class="form-control-static">{{newform.dataValues.Department}}</p>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="serial" class="control-label col-lg-2">Serial's Number:</label>
                                        <div class="col-lg-4">
